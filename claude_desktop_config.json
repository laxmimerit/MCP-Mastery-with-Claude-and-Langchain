			]
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		},
		"airbnb": {
			"command": "npx",
			"args": [
				"-y",
				"@openbnb/mcp-server-airbnb",
				"--ignore-robots-txt"
			]
		},
		"excel": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"--yes",
				"@negokaz/excel-mcp-server"
			],
			"env": {
				"EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
			}
		},
		"ppt": {
			"command": "uvx",
			"args": [
				"--from",
				"office-powerpoint-mcp-server",
				"ppt_mcp_server"
			],
